{
	"manifest_version": 2,
	"name": "Twitchがちょこっと便利になる",
	"version": "1.0",
	"description": "Twitchがちょこっと便利になります。",
	"browser_action": {
		"default_title": "Twitch＋",
		"default_popup": "popup/popup.html"
	},
	"content_scripts": [
		{
			"matches": ["https://www.twitch.tv/*"],
			"js": ["js/sidebar.js"]
		}
	]
}
